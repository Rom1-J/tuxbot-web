[MASTER]
load-plugins=pylint_django
disable=
	W0221,  # arguments-differ
	R0903,  # too-few-public-methods

[FORMAT]
max-line-length=79

[MESSAGES CONTROL]
disable=missing-docstring,invalid-name

[DESIGN]
max-parents=13

[TYPECHECK]
generated-members=REQUEST,acl_users,aq_parent,"[a-zA-Z]+_set{1,2}",save,delete
